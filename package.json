{
    "name": "plannable",
    "version": "9.0.0",
    "private": true,
    "description": "A website that helps UVa students schedule their classes more efficiently.",
    "author": {
        "email": "",
        "name": "Plannable",
        "url": "https://github.com/awesome-schedule/plannable"
    },
    "contributors": [
        {
            "email": "hz2zz@virginia.edu",
            "name": "Hanzhi Zhou",
            "url": "https://github.com/hanzhi713"
        },
        {
